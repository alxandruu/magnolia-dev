<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM "module.dtd" >
<module>
  <name>training-templating</name>
  <displayName>${project.name}</displayName>
  <description>${project.description}</description>
  <class>info.magnolia.training.fullstack.templating.TrainingTemplatingBasicModule</class>
  <versionHandler>info.magnolia.training.fullstack.templating.setup.TrainingTemplatingBasicModuleVersionHandler</versionHandler>
  <version>${project.version}</version>
  
  <dependencies>
    <dependency>
      <name>travel-demo</name>
      <version>1.5/*</version>
    </dependency>
    <dependency>
      <name>travel-demo-multisite</name>
      <version>1.5/*</version>
    </dependency>
    
    <!-- A good way to ensure, the whole EE package is installed. -->
    <dependency>
      <name>multisite</name>
      <version>2.1.1/*</version>
    </dependency>
  </dependencies>
  
</module>
